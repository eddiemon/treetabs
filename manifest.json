{
  "manifest_version": 2,

  "name": "TabTree",
  "description": "This extension adds a tree view of your open tabs!",
  "version": "0.1",

  "author": "Eddie Sundvall",
  "icons": {
    "32": "treeicon.png",
    "64": "treeicon.png"
  },

  "browser_action": {
    "default_icon": "treeicon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],

  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}